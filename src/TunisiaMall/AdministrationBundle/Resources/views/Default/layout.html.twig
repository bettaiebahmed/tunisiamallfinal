<!DOCTYPE html>
<html lang="en">
    <head>        
        <!-- META SECTION -->
        {% block title %} {%endblock%}           
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        
        <link rel="icon" href="favicon.ico" type="image/x-icon" />
        <!-- END META SECTION -->
        
        <!-- CSS INCLUDE -->        
        <link rel="stylesheet" type="text/css" id="theme" href="{{asset('administration/css/theme-default.css')}}"/>
        <!-- EOF CSS INCLUDE -->                                    
    </head>
    <body>
        <!-- START PAGE CONTAINER -->
        <div class="page-container">
            
            <!-- START PAGE SIDEBAR -->
            <div class="page-sidebar">
                <!-- START X-NAVIGATION -->
                <ul class="x-navigation">
                    <li class="">
                        <a href="index.html">TUNISIA MALL</a>
                        <a href="#" class="x-navigation-control"></a>
                    </li>
                    <li class="xn-profile">
                        <a href="#" class="profile-mini">
                            {% block photo_admin %} {% endblock %}
                        </a>
                       <div class="profile">
                            <div class="profile-image">
                                {% block deuxieme_photo %} {%endblock %}
                            </div>
                           
                            <div class="profile-data">
                                <div class="profile-data-name">{{app.user.username}}</div>
                                
                            </div>
                            
                        </div>                                                                        
                    </li>
                    <li class="xn-title">Navigation</li>
                    {% block navigation %} {% endblock %}
                    
                    
                </ul>
                <!-- END X-NAVIGATION -->
            </div>
            <!-- END PAGE SIDEBAR -->
            
            <!-- PAGE CONTENT -->
            <div class="page-content">
                
                <!-- START X-NAVIGATION VERTICAL -->
                <ul class="x-navigation x-navigation-horizontal x-navigation-panel">
                    <!-- TOGGLE NAVIGATION -->
                    <li class="xn-icon-button">
                        <a href="#" class="x-navigation-minimize"><span class="fa fa-dedent"></span></a>
                    </li>
                    <!-- END TOGGLE NAVIGATION -->
                    <!-- SEARCH -->
                     
                    <!-- END SEARCH -->
                    <!-- SIGN OUT -->
                    <li class="xn-icon-button pull-right">
                        <a href="#" class="mb-control" data-box="#mb-signout"><span class="fa fa-sign-out"></span></a>                        
                    </li> 
                    <!-- END SIGN OUT -->
                    
                </ul>
                <!-- END X-NAVIGATION VERTICAL -->                     

                <!-- START BREADCRUMB -->
                <ul class="breadcrumb">
                    <li><a href="#">Acceuil</a></li>                    
                    {% block emplacement %} {%endblock%}
                    
                </ul>
                <!-- END BREADCRUMB -->                       
                
                <!-- PAGE CONTENT WRAPPER -->
                {% block content %} {%endblock%}
            </div>            
            <!-- END PAGE CONTENT -->
        </div>
        <!-- END PAGE CONTAINER -->

        <!-- MESSAGE BOX-->
        <div class="message-box animated fadeIn" data-sound="alert" id="mb-signout">
            <div class="mb-container">
                <div class="mb-middle">
                    <div class="mb-title"><span class="fa fa-sign-out"></span> Se <strong>Déconnecter</strong> ?</div>
                    <div class="mb-content">
                        <p>Vous êtes sûr de déconnecter?</p>                    
                        <p>Appuyer sur Oui pour confirmer,Non pour annuler.</p>
                    </div>
                    <div class="mb-footer">
                        <div class="pull-right">
                            <a href="pages-login.html" class="btn btn-success btn-lg">Oui</a>
                            <button class="btn btn-default btn-lg mb-control-close">Non</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END MESSAGE BOX-->

        <!-- START PRELOADS -->
        <audio id="audio-alert" src="{{asset('audio/alert.mp3" preload="auto')}}"></audio>
        <audio id="audio-fail" src="{{asset('audio/fail.mp3')}}" preload="auto"></audio>
        <!-- END PRELOADS -->                  
        
    <!-- START SCRIPTS -->
        <!-- START PLUGINS -->
        <script type="text/javascript" src="{{asset('administration/js/plugins/jquery/jquery.min.js')}}"></script>
        <script type="text/javascript" src="{{asset('administration/js/plugins/jquery/jquery-ui.min.js')}}"></script>
        <script type="text/javascript" src="{{asset('administration/js/plugins/bootstrap/bootstrap.min.js')}}"></script>        
        <!-- END PLUGINS -->

        <!-- START THIS PAGE PLUGINS-->        
        <script type='text/javascript' src='{{asset('administration/js/plugins/icheck/icheck.min.js')}}'></script>        
        <script type="text/javascript" src="{{asset('administration/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js')}}"></script>
        <script type="text/javascript" src="{{asset('administration/js/plugins/scrolltotop/scrolltopcontrol.js')}}"></script>
        
        <script type="text/javascript" src="{{asset('administration/js/plugins/morris/raphael-min.js')}}"></script>
        <script type="text/javascript" src="{{asset('administration/js/plugins/morris/morris.min.js')}}"></script>       
        <script type="text/javascript" src="{{asset('administration/js/plugins/rickshaw/d3.v3.js')}}"></script>
        <script type="text/javascript" src="{{asset('administration/js/plugins/rickshaw/rickshaw.min.js')}}"></script>
        <script type='text/javascript' src='{{asset('administration/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js')}}'></script>
        <script type='text/javascript' src='{{asset('administration/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js')}}'></script>                
        <script type='text/javascript' src='{{asset('administration/js/plugins/bootstrap/bootstrap-datepicker.js')}}'></script>                
        <script type="text/javascript" src="{{asset('administration/js/plugins/owl/owl.carousel.min.js')}}"></script>                 
        
        <script type="text/javascript" src="{{asset('administration/js/plugins/moment.min.js')}}"></script>
        <script type="text/javascript" src="{{asset('administration/js/plugins/daterangepicker/daterangepicker.js')}}"></script>
        <!-- END THIS PAGE PLUGINS-->        

        <!-- START TEMPLATE -->
        <script type="text/javascript" src="{{asset('administration/js/settings.js')}}"></script>
        
        <script type="text/javascript" src="{{asset('administration/js/plugins.js')}}"></script>        
        <script type="text/javascript" src="{{asset('administration/js/actions.js')}}"></script>
        
        <script type="text/javascript" src="{{asset('administration/js/demo_dashboard.js')}}"></script>
        <!-- END TEMPLATE -->
    <!-- END SCRIPTS -->         
    </body>
</html>






